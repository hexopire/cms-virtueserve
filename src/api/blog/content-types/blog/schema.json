{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "content": {
      "type": "text"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "website": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::website.website",
      "inversedBy": "blogs"
    },
    "isPublishedAt": {
      "type": "datetime"
    },
    "context": {
      "type": "blocks",
      "required": true
    }
  }
}
